{
  "Globals": {

    "__comment" : "*** total worker threads ***",
    "ThreadPoolSize" : "10",

    "__comment" : "*** total caches in pool ***",
    "CachePoolSize" : "5",

    "__comment" : "*** this's the datanode group count, each transaction uses one group ***",
    "DatanodeGroupCount" : "3",

    "BindAddr" : "127.0.0.1",
    "ListenPort" : "2331"
  },

  "DataNodes": {
    "dn1": {
      "Address": "127.0.0.1:3306",
      "Schema": "orcl",
      "Auth": {"root":"123"}
    },
    "dn2": {
      "Address": "127.0.0.1",
      "Schema": "tsdb1",
      "Auth": {"root":"123"}
    },
    "dn3": {
      "Address": "127.0.0.1",
      "Schema": "tsdb0",
      "Auth": {"root":"123"}
    }
  },

