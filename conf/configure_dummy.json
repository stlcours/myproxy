          "Mappings": {
            "dn2": { "IoType": "read"}
          },
          "shardingKey": {
            "id": { "rule": "dummy" }
          }
        },
        "test_tbl": {
          /* "Mappings": {
            "dn1": { "IoType": "read"},
            "dn2": { "IoType": "read"}
          }, */
